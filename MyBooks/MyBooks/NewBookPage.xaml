<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyBooks.NewBookPage">
    <StackLayout Margin="10,10,10,0" >
        <Entry x:Name="nameEntry"  Placeholder= "Write the name of the game"/>
        <Entry  x:Name="publisherEntry" Placeholder="Write the publisher of the game"/>
        <Button Text="Scan" Clicked="Button_Clicked"/>
        <Button Text="Insert" Clicked="Button_Clicked_1"/>
       
    </StackLayout>
</ContentPage>